{
    "plugins": ["flexible-alerts","favicon-absolute", "sitemap", "video-embed", "reflinks","github",
    "chart",
    "todo",
    "graph",
    "puml",
    "katex",
    "mermaid-gb3",
    "expandable-chapters",
    "toggle-chapters",
    "chapter-fold",
    "theme-comscore",
    "theme-default",
    "highlight",
    "search",
    "sharing",
    "fontsettings",
    "language-selector"],
    "title": "CPBOX Documentation",
    "root": ".",
    "pluginsConfig": {
        "favicon-absolute":{
            "favicon": "/favicon.ico",
            "appleTouchIconPrecomposed152": "./favicon.ico"
        },
        "sitemap": {
            "hostname": "https://docs.cpbox.io"
        },
        "anchor-navigation-ex": {
            "showLevel": false,
            "showGoTop": true
        },
        "github": {
            "url": "https://gitub.com/cpbox"
        },
        "theme-default": {
            "showSummary": true
        },
        "language-selector": {
            "languages": [
                {"code": "en", "name": "English"},
                {"code": "zh", "name": "中文"}
            ],
            "defaultLanguage": "en",
            "label": {
                "en": "语言",
                "zh": "Language"
            }
        }
    },
    "structure": {
        "readme": "README.md",
        "summary": "SUMMARY.md"
    }
}
